
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

alias sgi="sudo apt-get install"
alias sgr="sudo apt-get remove"
alias sgar="sudo apt-get autoremove"
alias sgu="sudo apt-get update"
alias sgg="sudo apt-get upgrade"
alias sgdg="sudo apt-get dist-upgrade"

