[alias]
    c = clone
    g = gui
    i = init
    s = submodule
    ad = add
    aa = add --all
    br = branch
    bd = branch -d
    cf = config
    cg = config --global
    cl = clean
    cm = commit -v
    ca = commit -v -a
    co = checkout
    cp = cherry-pick
    df = diff
    dc = diff --cached
    dh = diff HEAD
    ds = diff --stat
    fc = fetch -v
    lg = log
    ls = log -1 HEAD
    ln = log            --numstat --all --decorate --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]"
    lp = log --graph --date=short --all --decorate --pretty=format:"%C(yellow)%h\\ %C(green)%ad%Cred%d\\ %Creset%s%Cblue\\ [%cn]"
    lr = log      --date=relative --all --decorate --pretty=format:"%C(green)%h\\ %C(yellow)[%ad]%Cred%d\\ %Creset%s%Cblue\\ [%cn]"
    mg = merge
    pl = pull -v
    pa = pull -v --all
    pr = pull -v --recurse-submodules
    ps = push -v
    rc = rm --cached
    rd = rm --dry-run
    re = remote
    rs = reset
    rf = reset --soft
    rh = reset --hard
    sa = stash
    sl = stash list
    sp = stash apply
    sd = stash drop
    st = status
    ss = status --short
    un = reset HEAD --
[color]
    diff = auto
    status = auto
    branch = auto
[push]
    default = simple
[http]
